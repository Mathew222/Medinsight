# Firebase Configuration
# Replace these placeholder values with your actual Firebase configuration
# You can find these values in your Firebase Console -> Project Settings -> General -> Your Apps -> SDK setup and configuration

# The API key for your Firebase project
VITE_FIREBASE_API_KEY=your-api-key

# The auth domain for your Firebase project (format: your-project-id.firebaseapp.com)
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com

# Your Firebase project ID
VITE_FIREBASE_PROJECT_ID=your-project-id

# Your Firebase storage bucket (format: your-project-id.appspot.com)
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com

# Your Firebase messaging sender ID (a number)
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id

# Your Firebase app ID (format: 1:123456789:web:abcdef)
VITE_FIREBASE_APP_ID=your-app-id 